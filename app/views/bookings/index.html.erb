<h1>My bookings</h1>
 <div class=container>
  <div class=row>
    <div class="col-12">
      <%@bookings.each do |booking|%>
      <%= booking.user.name %>
      <%= booking.user.title %>
      <%= booking.user.location %>
      <%= booking.start_date %>
      <%= booking.end_date %>
      <%end %>
    </div>
  </div>
 </div>
