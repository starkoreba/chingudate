<h1>My offers</h1>
<div class = container>
  <div class = row>
    <div class="col-12">
      <% @my_offers.each do |offer| %>
        <%= offer.title %>
    </div>
  </div>
  <div class="row">
    <% offer.bookings.each do |booking| %>
    <%= booking.status%>
        <% end %>
      <% end %>
  </div>
</div>
